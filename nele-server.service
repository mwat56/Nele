[Unit]
Description=Nele blogging server
After=network.target

[Service]
Type=simple
User=matthias
Group=matthias
WorkingDirectory=/home/matthias/devel/Go/src/github.com/mwat56/Nele/
ExecStart=/home/matthias/devel/Go/src/github.com/mwat56/Nele/nele-linux-amd64 -ini=blog.ini -certKey="" -certPem="" -hashfile=hashfile.db -listen="192.168.192.234" -port=8181 -log=access.bla.mwat.de -uf=pwaccess.db
Restart=on-failure

[Install]
WantedBy=multi-user.target
